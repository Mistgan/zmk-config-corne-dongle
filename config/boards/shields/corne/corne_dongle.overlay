#include "corne.dtsi"

/ {
    chosen {
        zmk,kscan = &mock_kscan;
        zmk,underglow = &mock_underglow;
        wakeup-source;
    };

    mock_kscan: kscan_1 {
        compatible = "zmk,kscan-mock";
        columns = <0>;
        rows = <0>;
        events = <0>;
    };
};

&mock_underglow {
    compatible = "zmk,underglow-mock";
    num-leds = <0>;
    leds = <0>;
};
